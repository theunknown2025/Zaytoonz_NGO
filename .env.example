# Database
DATABASE_URL="your_database_url_here"
NEXT_PUBLIC_SUPABASE_URL="your_supabase_url"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your_supabase_anon_key"

# Authentication
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your_nextauth_secret"

# Python Scraper Configuration (existing)
NEXT_PUBLIC_USE_EXTERNAL_SCRAPER=true
NEXT_PUBLIC_EXTERNAL_SCRAPER_URL=http://localhost:8000
NEXT_PUBLIC_FALLBACK_TO_LOCAL=true
NEXT_PUBLIC_PREFERRED_AI_MODEL=gpt-4o-mini
NEXT_PUBLIC_ENABLE_PAGINATION=true
NEXT_PUBLIC_PAGINATION_DETAILS=""

# RSS.app Integration
RSS_APP_API_KEY="your_rss_app_api_key"
RSS_APP_API_SECRET="your_rss_app_api_secret"

# Optional: RSS Feed Configuration
RSS_FEED_AUTO_REFRESH_INTERVAL=3600000  # 1 hour in milliseconds
RSS_FEED_MAX_ITEMS_PER_IMPORT=50

# OpenAI Configuration for Zaytoonz AI Scraper
OPENAI_API_KEY="your_openai_api_key_here"
OPENAI_MODEL="gpt-4o-mini"
OPENAI_MAX_TOKENS="2000" 